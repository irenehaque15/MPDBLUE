<% form_for :dump, :as => :team, :url=>{:controller=>"students", :action=>"csv_import"}, :html => { :multipart => true } do |form| %>
	<table">
		<tr>
			<td>
				<label for="team_file">
					Select a CSV File :
				</label>
			</td>
			<td >
				<%= form.file_field :file -%>
			</td>
		</tr>
		<tr>
			<td colspan='2'>
				<%= submit_tag 'Submit' -%>
			</td>
		</tr>
	</table>
<% end -%>
